services:
  botenodk:
    build:
      context: C:\repos\BotenODK\BotenODK
      dockerfile: BotenODK/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BotenODK
      target: base
    container_name: BotenODK_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: botenodk:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/BotenODK.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\repos\BotenODK\BotenODK\BotenODK:/app:rw
    - C:\Users\elange\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\elange\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\elange\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\elange\.nuget\packages:/root/.nuget/packages:ro
    - C:\repos\BotenODK\BotenODK:/src:rw
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: L@ngendijk11
    image: mcr.microsoft.com/mssql/server:2019-latest
version: '3.4'